<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<!-- ================== USER MODULE ================== -->
	
	<bean id="springUserAction" class="framgiavn.project01.web.action.UserAction"
		scope="prototype">
		<property name="userBusiness" ref="UserBusiness" />
	</bean>

	<bean id="springPlayerAction" class="framgiavn.project01.web.action.PlayerAction" scope="prototype">
		<property name="playerBusiness" ref="playerBusiness"></property>
	</bean>
</beans>